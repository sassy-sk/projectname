<%# サービス説明を入力 %>
<% const description = 'サービス説明を入力'; %>

<meta charset="utf-8">
<title><%= title %></title>

<meta name="description" content="<%=description%>">
<meta property="og:title" content="<%= title %>">
<meta property="og:site_name" content="<%= title %>">
<meta property="og:description" content="<%=description%>">
<meta property="og:type" content="website">
<meta property="og:url" content="<%=path%>">
<meta property="og:image" content="<%=path%>assets/images/ogp.png">
<meta property="og:locale" content="ja_JP">

<meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">

<meta name="format-detection" content="telephone=no">

<!-- ファビコン -->
<link rel="icon" href="<%=path%>assets/images/favicon.ico">

<!-- スマホ用アイコン -->
<link rel="apple-touch-icon" sizes="180x180" href="<%=path%>assets/images/apple-touch-icon.png">

<!-- Windows用アイコン -->
<meta name="application-name" content="<%= title %>" />
<meta name="msapplication-square70x70logo" content="<%=path%>assets/images/70x70.png" />
<meta name="msapplication-square150x150logo" content="<%=path%>assets/images/150x150.png" />
<meta name="msapplication-wide310x150logo" content="<%=path%>assets/images/webicon_wide.png" />
<meta name="msapplication-square310x310logo" content="<%=path%>assets/images/310x310.png" />
<!-- ▼デザイナーさんに確認し、カラーの最適化が必要 -->
